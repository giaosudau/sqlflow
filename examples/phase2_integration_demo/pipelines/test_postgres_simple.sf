-- Simple PostgreSQL connector test to debug column issues

SOURCE test_customers TYPE POSTGRES PARAMS {
    "host": "postgres",
    "port": 5432,
    "database": "demo",
    "username": "sqlflow",
    "password": "sqlflow123",
    "table": "customers"
};

LOAD test_customers_table FROM test_customers;

-- Test to access specific columns by name
CREATE TABLE debug_test AS
SELECT customer_id, first_name, last_name FROM test_customers_table LIMIT 1;

-- Export debug results
EXPORT SELECT * FROM debug_test
TO 'output/debug_test.csv'
TYPE CSV
OPTIONS { "header": true }; 